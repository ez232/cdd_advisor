<h1>Listing labels</h1>

<table>
  <tr>
    <th>Proposal</th>
    <th>Printed</th>
    <th>Digital</th>
    <th>Font type</th>
    <th>Text size</th>
    <th>Text colour</th>
    <th>Labels reading distance</th>
    <th>Background colour</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @labels.each do |label| %>
  <tr>
    <td><%= label.proposal_id %></td>
    <td><%= label.printed %></td>
    <td><%= label.digital %></td>
    <td><%= label.font_type %></td>
    <td><%= label.text_size %></td>
    <td><%= label.text_colour %></td>
    <td><%= label.labels_reading_distance %></td>
    <td><%= label.background_colour %></td>
    <td><%= link_to 'Show', label %></td>
    <td><%= link_to 'Edit', edit_label_path(label) %></td>
    <td><%= link_to 'Destroy', label, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Label', new_label_path %>
