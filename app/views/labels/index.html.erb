<h1>Listing labels</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
  </tr>

<% @labels.each do |label| %>
  <tr>
    <td><%= label.name %></td>
    <td><%= link_to 'Edit',
          edit_project_proposal_label_path(@project, @proposal, label) %></td>
    <td><%= link_to 'Remove', [@project, @proposal, label], method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to '&laquo; Back'.html_safe, :back %>
<%= link_to 'New Label', new_project_proposal_label_path(@project, @proposal) %>
