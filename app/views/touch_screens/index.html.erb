<h1>Listing touch_screens</h1>

<table>
  <tr>
    <th>Proposal</th>
    <th>Button shape</th>
    <th>Button length or radius</th>
    <th>Button width</th>
    <th>Button height</th>
    <th>Scroll bar shape</th>
    <th>Scroll bar length or radius</th>
    <th>Scroll bar width</th>
    <th>Scroll bar height</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @touch_screens.each do |touch_screen| %>
  <tr>
    <td><%= touch_screen.proposal_id %></td>
    <td><%= touch_screen.button_shape %></td>
    <td><%= touch_screen.button_length_or_radius %></td>
    <td><%= touch_screen.button_width %></td>
    <td><%= touch_screen.button_height %></td>
    <td><%= touch_screen.scroll_bar_shape %></td>
    <td><%= touch_screen.scroll_bar_length_or_radius %></td>
    <td><%= touch_screen.scroll_bar_width %></td>
    <td><%= touch_screen.scroll_bar_height %></td>
    <td><%= link_to 'Show', touch_screen %></td>
    <td><%= link_to 'Edit', edit_touch_screen_path(touch_screen) %></td>
    <td><%= link_to 'Destroy', touch_screen, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Touch screen', new_touch_screen_path %>
